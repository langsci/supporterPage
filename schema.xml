<schema version="0.2">

	<!--
	 *
	 * TABLE langsci_prominent_users
	 *
	 -->
	<table name="langsci_prominent_supporters">
		<field name="prominent_id" type="I8">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="user_id" type="I8">
		</field>
		<index name="supporter_page_settings_user_id">
			<col>user_id</col>
			<UNIQUE />
		</index>
		<descr>List of prominent supporters</descr>
	</table>

</schema>



























